{
  "plugins": [
    "react-native-web/babel",
    "transform-class-properties",
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src"
        }
      }
    ]
  ],
  "presets": [
    "stage-2",
    "react-native",
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": ["last 2 Chrome versions"]
        }
      }
    ]
  ]
}
